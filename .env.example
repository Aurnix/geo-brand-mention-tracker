# Database
DATABASE_URL=postgresql+asyncpg://geotrack:geotrack@db:5432/geotrack
DATABASE_URL_SYNC=postgresql://geotrack:geotrack@db:5432/geotrack
POSTGRES_USER=geotrack
POSTGRES_PASSWORD=geotrack
POSTGRES_DB=geotrack

# Auth
JWT_SECRET=change-me-to-a-real-secret-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=1440

# AI Engine API Keys
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
PERPLEXITY_API_KEY=pplx-...
GOOGLE_AI_API_KEY=...

# Parser (for cheap LLM calls used in response parsing)
PARSER_MODEL=gpt-4o-mini
PARSER_PROVIDER=openai

# CORS (comma-separated origins)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Scheduler
RUN_SCHEDULE_HOUR=3
RUN_SCHEDULE_MINUTE=0

# Frontend
NEXTAUTH_SECRET=change-me-to-a-real-secret-in-production
NEXTAUTH_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:8000
